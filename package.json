{
  "name": "queenjeannes",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf lib",
    "prepare": "npx snowpack",
    "sass": "node-sass src/styles/global.scss lib/styles/global.css",
    "sass:watch": "node-sass --watch src/styles/global.scss lib/styles/global.css",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "build": "npm run tsc && npm run sass",
    "serve": "live-server",
    "prestart": "npm run clean && npm run sass",
    "start": "concurrently \"npm run sass:watch\" \"npm run tsc:watch\" \"npm run serve\""
  },
  "author": "Andrew Petersen",
  "dependencies": {
    "react": "npm:@pika/react",
    "react-dom": "npm:@pika/react-dom"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "rimraf": "^3.0.2",
    "snowpack": "^1.6.0",
    "typescript": "^3.8.3"
  }
}
